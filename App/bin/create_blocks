#!/usr/bin/env bash
# vim: tabstop=4:

#------------------------------------------------------------------------------------------
    _DEPLOY_VERSION=1.0.1-01

#------------------------------------------------------------------------------------------
    _THIS="$0"
    [[ -h "$_THIS" ]] && _THIS="$(readlink "$_THIS")"

    _HERE="$(cd $(dirname "$_THIS"); pwd)"
    _SELF="$(basename "$0")"
    _THIS="$PWD/$_THIS"

    echo THIS=$_THIS
    echo HERE=$_HERE
    echo SELF=$_SELF

#------------------------------------------------------------------------------
    BLOCKS="$(sed -n '/^\#DATA/,$p' "$_THIS" | sed -n '2,$p')"

    for BLOCK in $BLOCKS
    do
        echo "ionic generate page    block/${BLOCK,,}/pages/$BLOCK"
        echo "ionic generate service block/${BLOCK,,}/service/$BLOCK"
    done

#==============================================================================
    exit

#DATA===========================================================================
